{
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": "airbnb-base/legacy",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "no-underscore-dangle":"off"
    }
}