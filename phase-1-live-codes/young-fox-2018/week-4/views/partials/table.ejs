<table class="table">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Title</th>
      <th scope="col">Duration</th>
      <th scope="col">Rating</th>
      <th scope="col">Released Date</th>
    </tr>
  </thead>
  <tbody>
    <%
      data.forEach((datum, index) => { %>
        <tr>
          <th scope="row"><%= index + 1 %></th>
          <td><a href="/movies/<%=datum.id%>"><%= datum.title %></a></td>
          <td><%= durationConverter(datum.duration) %></td>
          <td><%= datum.rating %></td>
          <td><%= dayConverter(datum.createdAt) %></td>
        </tr>
      <%})
    %>
  </tbody>
</table>
