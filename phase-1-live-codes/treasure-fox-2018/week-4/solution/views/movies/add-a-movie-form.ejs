<h2>Add a Movie</h2>
<%- include('../partials/validation-errors') %>
<form action="/movies" method="post">
  Title
  <br>
  <input type="text" name="title" class="field" autocomplete="off"
  autofocus>
  <br>
  Description
  <br>
  <input type="text" name="description" class="field" autocomplete="off">
  <br>
  Rating
  <br>
  <input type="text" name="rating" class="field no-mb" autocomplete="off">
  <br>
  <input type="submit" class="button">
</form>
